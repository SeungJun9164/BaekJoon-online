#include <iostream>

using namespace std;

int main()
{
    int n, cnt[80] = { 0 }, sumCnt;
    string str;

    cin >> n;
    for (int i = 0; i < n; i++)
    {
        cin >> str;

        if (str[0] == 'O' || str[0] == 'o')
        {
            cnt[0] = 1;
            sumCnt = 1;
        }
        else
        {
            cnt[0] = 0;
            sumCnt = 0;
        }

        for (int j = 1; j < str.length(); j++)
        {
            if (str[j] == 'O' || str[j] == 'o')
            {
                if (str[j - 1] == 'O' || str[j - 1] == 'o')
                    cnt[j] = cnt[j - 1] + 1;
                else
                    cnt[j] = 1;
            }
            else
                cnt[j] = 0;
            sumCnt += cnt[j];
        }
        cout << sumCnt << "\n";
    }
}
//동적할당 방식으로 풀려고하니 너무 어렵고 2시간이 지나도록 해결하지 못해 블로그에 나와있는 코드를 보고 문제를 풀었다.
//내가 문제를 너무 어렵게 접근하는것 같다고 느껴졌다. (https://blog.naver.com/jmk1118/221817010057 <<도움 받은 곳)

-----------------------------------------------------------------------------------------------------------------
#include <iostream>

using namespace std;

int main()
{
    cout << fixed;
    cout.precision(3);

    int c, n, sum = 0, cnt = 0; //c : 테스트 갯수 n : 학생수
    int score[1000];
    double avg, rat;

    cin >> c;
    for (int i = 0; i < c; i++)
    {
        rat = 0;
        cnt = 0;
        sum = 0;
        cin >> n;
        for (int j = 0; j < n; j++)
        {
            cin >> score[j];
            sum += score[j];
        }
        avg = (double)sum / n;
        for (int k = 0; k < n; k++)
        {
            if (avg < score[k])
                cnt++;
        }
        rat = (double)cnt / n * 100;
        cout << rat << "%\n";
    }
}
