#include <iostream>

using namespace std;

int main()
{
	int x, y;
	cin >> x >> y;

	if (x > 0 && y > 0)
		cout << 1;
	else if (x < 0 && y < 0)
		cout << 3;
	else if (x > 0 && y < 0)
		cout << 4;
	else if (x < 0 && y > 0)
		cout << 2;
	return 0;
}
-----------------------------------------------------
#include <iostream>

using namespace std;

int main()
{
	int h, m; //h : 시간 m : 분
	cin >> h >> m;	
	if (m >= 45)
		cout << h << " " << m - 45;
	else if (h == 0)
		cout << 23 << " " << 60 - (45 - m);
	else
		cout << h - 1 << " " << 60 - (45 - m);
	return 0;
}
