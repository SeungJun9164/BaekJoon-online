#include<iostream>

using namespace std;

int main()
{
    int a,b;
    while(true)
    {
        cin>>a>>b;
        if(a==0 && b==0)
            break;
        else
            cout<<a+b<<"\n";
    }
    return 0;
}

--------------------------------------------------------------------
#include<iostream>

using namespace std;

int main()
{
    int a,b;
    while(cin>>a>>b)
    {   
        cout<<a+b<<"\n";       
    }
    return 0;
}
//아무런 조건이 없어서 처음에 무한루프에 걸려 시간초과가 났지만 while문의 조건을 입력이 들어올때만으로 바꿔서 해결했다.
------------------------------------------------------------------
#include<iostream>

using namespace std;

int main()
{
    int n, sum, x=0,y, cnt = 0;
    cin >> n;
    y = n;
    while (true)
    {
        sum = (y / 10) + (y % 10);
        x = (y % 10) * 10 + (sum % 10);
        cnt++;
        y = x;
        if (y == n)
            break;
    }
    cout << cnt;

    return 0;
}

