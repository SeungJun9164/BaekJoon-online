#include <iostream>

bool mat[2190][2190] = { 0 };

void star(int r, int c, int size) {
	if (size == 3) { 
		if(!mat[r + 1][c + 1]) mat[r + 1][c + 1] = 1; //가운데 공백 지정
		return;
	}
	for (int i = r + size / 3; i < r + size / 3 * 2; i++)  //정가운데 공백 지정
		for (int j = c + size / 3; j < c + size / 3 * 2; j++) 
			if (!mat[i][j]) mat[i][j] = 1;
	
	for (int i = 0; i < 3; i++) {
		for (int j = 0; j < 3; j++) {
			if (i == 1 && j == 1) continue;
			star(r + i * size / 3, c + j * size / 3, size / 3); //9칸으로 쪼개서 재귀함수 호출
		}
	}
}

int main() {
	int n;
	std::cin >> n;
	star(0,0,n);
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			if (mat[i][j]) std::cout<<" ";
			else std::cout<<"*";
		}
		std::cout<<"\n";
	}
}
//문제 패턴을 찾지 못해 다른 사람의 코드를 보고 공부하였다. 그런데도 아직까지 패턴이 이해가 잘 안된다. 다시 봐야겠다
