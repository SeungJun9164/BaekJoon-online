#include <iostream>
using namespace std;

int main()
{
	int k, n, t, sum[15][15];
	
	
	for (int i = 0; i < 15; i++)
	{
		sum[0][i] = i + 1;
		sum[i][0] = 1;
	}
	for (int i = 1; i < 15; i++)
	{
		for (int j = 1; j < 14; j++)
		{
			sum[i][j] = sum[i - 1][j] + sum[i][j - 1];
		}
	}
	cin >> t;
	for (int i = 0; i < t; i++)
	{
		cin >> k >> n;
		cout << sum[k][n - 1] << "\n";
	}
}
