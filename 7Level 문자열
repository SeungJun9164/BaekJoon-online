#include <iostream>

using namespace std;

int main()
{
	int n, sum=0;
	string str;
	char c;
	cin >> n;
	cin >> str;
	for (int i = 0; i < str.size(); i++)
	{
		sum += str[i] - '0';
	}
	cout << sum;
	return 0;
}
